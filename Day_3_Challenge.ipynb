    "def find_duplicates(num):\n",
    "    num.sort()\n",
    "    for i in range(1, len(num)):\n",
    "        if num[i]==num[i-1]:\n",
    "            return num[i]\n",
    "    return None\n",
    "\n",
    "arr_num=[1, 3, 4, 2, 5, 3]\n",
    "print(find_duplicates(arr_num))\n"
   ]
  }
